{
  "name": "@wyxos/vibe",
  "version": "1.3.1",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && node write-cname.js",
    "build:demo": "vue-tsc --noEmit && vite build && node write-cname.js",
    "build:lib": "vite build --config vite.lib.config.ts && vue-tsc --declaration --emitDeclarationOnly --outDir lib",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "release": "node release.mjs",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.15",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@types/node": "^24.5.2",
    "chalk": "^5.3.0",
    "inquirer": "^10.1.8",
    "jsdom": "^26.0.0",
    "simple-git": "^3.27.0",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "vite": "^6.2.0",
    "vite-plugin-vue-mcp": "^0.3.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^3.1.0"
  }
}
