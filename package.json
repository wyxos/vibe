{
  "name": "@wyxos/vibe",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build -c vite.config.lib.js",
    "build:types": "vue-tsc -p tsconfig.lib.json",
    "preview": "vite preview",
    "check": "npm -s run lint && npm -s run typecheck && npm -s run test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --reporter=dot --silent",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "typecheck": "vue-tsc --noEmit --pretty false",
    "postinstall": "playwright install chromium"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@tailwindcss/vite": "^4.1.18",
    "@eslint/js": "^9.37.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "jsdom": "^26.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "7.1.11",
    "vitest": "^2.1.8",
    "vue-eslint-parser": "^10.0.0",
    "vue-tsc": "^3.2.1"
  }
}
