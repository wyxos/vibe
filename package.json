{
  "name": "@wyxos/vibe",
  "version": "1.6.16",
  "description": "A high-performance, responsive masonry layout engine for Vue 3 with built-in infinite scrolling and virtualization.",
  "keywords": [
    "vue",
    "vue3",
    "masonry",
    "grid",
    "layout",
    "infinite-scroll",
    "virtualization",
    "responsive",
    "performance",
    "swipe",
    "feed",
    "mobile"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wyxos/vibe.git"
  },
  "homepage": "https://wyxos.github.io/vibe/",
  "bugs": {
    "url": "https://github.com/wyxos/vibe/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./vibe.css": "./lib/vibe.css",
    "./package.json": "./package.json"
  },
  "bin": {
    "vibe": "./toggle-link.mjs"
  },
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "sideEffects": [
    "./lib/vibe.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && node write-cname.js",
    "build:demo": "vue-tsc --noEmit && vite build && node write-cname.js",
    "build:lib": "vite build --config vite.lib.config.ts && vue-tsc --declaration --emitDeclarationOnly --outDir lib",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "release": "npx @wyxos/zephyr --type=node",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "@types/highlight.js": "^9.12.4",
    "highlight.js": "^11.11.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "vue": "^3.0.0",
    "vue-router": "^4.6.3"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.0.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "chalk": "^5.3.0",
    "inquirer": "^10.1.8",
    "jsdom": "^26.0.0",
    "sharp": "^0.34.5",
    "simple-git": "^3.27.0",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.9",
    "vue-tsc": "^3.1.0"
  }
}
